{
    "hostRequirements": {
        "cpus": 4
    },
    "updateContentCommand": "npm install",
    "postAttachCommand": "npm run dev",
    "customizations": {
        "codespaces": {
            "openFiles": [
                "src/pages/app.tsx"
            ]
        },
        "vscode": {
            "extensions": [
                "dbaeumer.vscode-eslint",
                "github.vscode-pull-request-github",
                "eamodio.gitlens",
                "christian-kohler.npm-intellisense"
            ]
        }
    },
    "forwardPorts": [3000],
    "portsAttributes": {
        "3000": {
          "label": "Application",
          "onAutoForward": "openPreview"
        }
        // "6006": {
        //     "label": "Storybook",
        //     "onAutoForward": "openBrowser"
        // }
      }
}